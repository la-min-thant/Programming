#include <stdio.h>
int main(){
    char myGrade='A';
    printf("%c,\n",myGrade);
    //try this
    char a=65, b=66, c=67;
    printf("%c\n",a);
    printf("%c\n",b);
    printf("%c\n",c);
    //that won't be show 65,66 and 67 because the format of char %c will be change automatically, 65 is A in char, 66 is B and 67 is Cin char format
    //if you want more than one word type this 
    char myText[]="Hello, fuck off";
    printf("%s",myText);
    //call string


    return 0; 
}