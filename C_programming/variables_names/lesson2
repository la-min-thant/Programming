#include <stdio.h>
int main(){
    int length=4;
    int width=6;
    int area;
    area=length*width;
    printf("Length is :%d.\n",length);
    printf("width is :%d.\n",width);
    printf("Area is :%d.\n",area);
    // try this 
    printf("The length is %dm, the width is %dm and the Area is %d square meter.\n",length,width,length*width);
    return 0;
}